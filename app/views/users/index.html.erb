<div>
    <% @users.each do |user| %>
        <div class="content">
            <p><%= user.id %></p>
            <p><%= user.email %></p>
            <p><%= user.password %></p>
            <p><%= user.created_at.strftime('%Y年%m月%d日%H時%M分') %></p>
            <p><%= link_to '詳細', user %></p>
            <p><%= link_to '削除', user, method: :delete %></p>
        </div>
    <% end %>
</div>